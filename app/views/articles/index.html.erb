<h1>Articles#index</h1>
<p>Find me in app/views/articles/index.html.erb</p>
<% @articles.each do |article| %>
  <h3><%= link_to article.title, article_path(article.id) %></h3>
  <h4><%= link_to article.content, article_path(article.id) %></h4>
  <%= link_to "View", article_path(article.id), class: 'btn btn-primary' %>
<% end %>
<% @categories.each do |category| %>
  <%= link_to category.category, articles_path(:cate => category.id) %>
  <%= category.articles.count %>
<% end %>
